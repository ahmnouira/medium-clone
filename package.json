{
  "name": "medium-clone",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "serve": "nodemon",
    "format": "prettier --check \"**/*.{ts,tsx}\" && import-sort --list-different --version \"**/*.{tsx,ts}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx}\" && import-sort --write \"**/*.{tsx,ts}\"", 
    "clean": "yarn cache clean && yarn install",
    "dev": "concurrently --kill-others \"yarn serve\" \"next\""
  },
  "dependencies": {
    "@types/styled-components": "^5.1.4",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "next": "^10.0.2",
    "next-redux-wrapper": "^6.0.2",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "styled-components": "^5.2.1",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.8",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.56",
    "@types/react-redux": "^7.1.11",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.7",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.5"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "option": {}
    }
  }
}
